#include<bits/stdc++.h>
using namespace std;
int main()
{
    int n;
    cin>>n;
    string A,B;
    cin>>A>>B;
    int s=0;

    for(int i=0;i<n;i++)
    {

        if(abs(A[i]-B[i])>5)
            s += 10-abs(A[i]-B[i]);
        else
        s +=  abs(A[i]-B[i]);

    }
    cout<<s;
    return 0;
}
