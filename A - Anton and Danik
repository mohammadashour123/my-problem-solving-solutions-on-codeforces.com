#include <iostream>
#include<string>
using namespace std;
 
int main()
{
 
   int a=0 , d=0 ; // \amount of characters A and D
   int n ; // refers to gmaes played
  // cout << "enter number of times "<<endl ;
   cin >> n ;
   string upper ; // refers to AAADDDAD
  // cout << "enter samples "<<endl ;
   cin>> upper ;
   for (int i = 0 ; i<n ; i++)
   {
       if (upper[i]=='A')
         a++ ;
       else
         d++ ;
   }
   if (a>d)
   {
       cout << "Anton" <<endl;
   }
     else if (a<d)
   {
       cout << "Danik" <<endl;
   }
     else
   {
       cout << "Friendship" <<endl;
   }
     return 0 ;
}
