#include <iostream>
#include <algorithm>
 
using namespace std;
 
int main()
{
    int a, b, c=0, v=0, u=0;
    cin >> a >> b;
    for(int i=1; i<=6; i++){
        if(abs(a-i)>abs(b-i)){
            c++;
        }
        if(abs(a-i)<abs(b-i)){
            v++;
        }
        if(abs(a-i)==abs(b-i)){
            u++;
        }
    }
    cout << v << " " << u << " " << c;
    return 0;
}
