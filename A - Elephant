#include <iostream>

using namespace std;

int main()
{
    int x,c=0;
    cin >> x;
    if(x>=5){
        c=x/5;
        x=x%5;
    }
    if(x>=4){
        c+=(x/4);
        x=x%4;
    }
    if(x>=3){
        c+=(x/3);
        x=x%3;
    }
    if(x>=2){
        c+=(x/2);
        x=x%2;
    }
    if(x>=1){
        c+=(x/1);
        x=x%1;
    }
    cout << c;
    return 0;
}
