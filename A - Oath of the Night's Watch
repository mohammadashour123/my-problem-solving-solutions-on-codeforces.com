#include <iostream>
#include<vector>
#include<algorithm>
using namespace std;
 
int main()
{
    int n , countt = 0 ;
    cin >> n ;
    vector<int> v(n) ;
 
    for(int i = 0 ; i<n ; i++){
        cin >> v[i];
 
 
            }
            sort(v.begin(),v.end()) ;
         for(int j = 0 ; j<n ; j++){
 
            if (v[0]< v[j]  && v[n-1]> v[j])
                countt++ ;
 
        }
 
    cout<<countt ;
    return 0;
}
