#include <iostream>

using namespace std;

int main()
{
    // vowels A E I O U Y W
    string s;
    cin >> s;

    for(int i=0; i<s.size(); i++){
        if(s[i] >= 65 && s[i] <=90 )
        {
            s[i]+=32;
        }

        if(s[i]=='a' || s[i]=='o'|| s[i]=='y'|| s[i]=='e'||s[i]=='u'||s[i]=='i')
        {
            continue;
        }
        else{
            cout << '.' << s[i] ;
        }
    }
    return 0;
}
